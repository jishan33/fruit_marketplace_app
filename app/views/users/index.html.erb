

<h1 class="mb-2 mt-2">Users </h1>


<div class="row col-12 d-flex justify-content-center mt-3">

   <table class="table">
    <thead class="thead-dark">
      <tr>
        <th scope="col">Username</th>
        <th scope="col">Email</th>
        <th scope="col">User Type</th>
        <th scope="col"></th>
      </tr>
    </thead>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.user_type %></td>
          
      
        <td><%= link_to 'Destroy', user_table_path + "/#{user.id}",method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </table>
</div>


